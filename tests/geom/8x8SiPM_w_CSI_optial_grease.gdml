<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<gdml xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="http://service-spi.web.cern.ch/service-spi/app/releases/GDML/schema/gdml.xsd">

  <define>
    <!-- Constants -->
    <constant name="pixel_width" value="2.0"/>
    <constant name="reflector_thickness" value="0.2"/>
    <constant name="pitch" value="2.2"/>
    <constant name="xtal_thickness" value="8.0"/>
    <constant name="sipm_size" value="2.0"/>
    <constant name="sipm_dead" value="0.2"/>
    <constant name="grease_thickness" value="0.1"/>
    <constant name="window_thickness" value="0.1"/>
    <constant name="array_size" value="8"/>
    
    <!-- Total array width calculation -->
    <constant name="xtal_array_width" value="17.4"/>
    <constant name="sipm_array_width" value="17.4"/>
    <constant name="startX" value="-8.7"/>
    <constant name="startY" value="-8.7"/>
    
    <!-- Z positions  (crystal bottom at z=0, top at z=8.0mm) -->
    <constant name="crystal_bottom_z" value="0.0"/>
    <constant name="crystal_top_z" value="8.0"/>
    <constant name="grease_bottom_z" value="8.0"/>
    <constant name="grease_top_z" value="8.1"/>
    <constant name="window_bottom_z" value="8.1"/>
    <constant name="window_top_z" value="8.2"/>
    <constant name="sipm_bottom_z" value="8.2"/>
    <constant name="sipm_top_z" value="8.4"/>
    
    <!-- Optical properties -->
    <matrix coldim="2" name="RINDEX_CRYSTAL" values="1.0*eV 1.82 4.0*eV 1.82"/>
    <matrix coldim="2" name="RINDEX_GREASE" values="1.0*eV 1.47 4.0*eV 1.47"/>
    <matrix coldim="2" name="RINDEX_WINDOW" values="1.0*eV 1.55 4.0*eV 1.55"/>
    <matrix coldim="2" name="RINDEX_AIR" values="1.0*eV 1.0 4.0*eV 1.0"/>
    
    <!-- Absorption lengths -->
    <matrix coldim="2" name="ABSLENGTH_CRYSTAL" values="1.0*eV 400.0 4.0*eV 400.0"/>
    <matrix coldim="2" name="ABSLENGTH_GREASE" values="1.0*eV 1000.0 4.0*eV 1000.0"/>
    <matrix coldim="2" name="ABSLENGTH_WINDOW" values="1.0*eV 1000.0 4.0*eV 1000.0"/>
    <matrix coldim="2" name="ABSLENGTH_AIR" values="1.0*eV 10000.0 4.0*eV 10000.0"/>
    
    <!-- Scintillation properties -->
    <matrix coldim="2" name="SCINT_SPECTRUM" values="1.0*eV 0.1 2.9*eV 1.0 4.0*eV 0.1"/>
    <!-- Const properties must be coldim="1" for Geant4 11.x -->
    <matrix coldim="1" name="SCINT_YIELD" values="5000.0"/>
    <matrix coldim="1" name="RESOLUTION_SCALE" values="1.0"/>
    <matrix coldim="1" name="FAST_TIME_CONST" values="21.5"/>
    <matrix coldim="1" name="SLOW_TIME_CONST" values="43.8"/>
    <matrix coldim="1" name="YIELD_RATIO" values="0.87"/>
    <!-- Reemission probability for Opticks U4Scint -->
    <matrix coldim="2" name="REEMISSION_PROB" values="1.0*eV 0.0 4.0*eV 0.0"/>
    
    <!-- Surface properties -->
    <matrix coldim="2" name="EFFICIENCY_0" values="1.0*eV 0.0 4.0*eV 0.0"/>
    <matrix coldim="2" name="EFFICIENCY_100" values="1.0*eV 1.0 4.0*eV 1.0"/>
    <matrix coldim="2" name="REFLECTIVITY_0" values="1.0*eV 0.0 4.0*eV 0.0"/>
    <matrix coldim="2" name="REFLECTIVITY_30" values="1.0*eV 0.3 4.0*eV 0.3"/>
    <matrix coldim="2" name="REFLECTIVITY_98" values="1.0*eV 0.98 4.0*eV 0.98"/>
  </define>

  <materials>
    <!-- Air -->
    <material name="Air" state="gas">
      <D unit="g/cm3" value="0.0012"/>
      <fraction n="0.7" ref="N"/>
      <fraction n="0.3" ref="O"/>
      <property name="RINDEX" ref="RINDEX_AIR"/>
      <property name="ABSLENGTH" ref="ABSLENGTH_AIR"/>
    </material>

    <!-- Crystal (CsI)  -->
    <material name="Crystal" state="solid">
      <D unit="g/cm3" value="4.51"/>
      <fraction n="1.0" ref="G4_CESIUM_IODIDE"/>
      <property name="RINDEX" ref="RINDEX_CRYSTAL"/>
      <property name="ABSLENGTH" ref="ABSLENGTH_CRYSTAL"/>
      <!-- Const properties (coldim="1" refs for Geant4 11.x) -->
      <property name="SCINTILLATIONYIELD" ref="SCINT_YIELD"/>
      <property name="RESOLUTIONSCALE" ref="RESOLUTION_SCALE"/>
      <!-- New Geant4 11.x style names -->
      <property name="SCINTILLATIONCOMPONENT1" ref="SCINT_SPECTRUM"/>
      <property name="SCINTILLATIONCOMPONENT2" ref="SCINT_SPECTRUM"/>
      <property name="SCINTILLATIONTIMECONSTANT1" ref="FAST_TIME_CONST"/>
      <property name="SCINTILLATIONTIMECONSTANT2" ref="SLOW_TIME_CONST"/>
      <!-- Old-style names for Opticks U4Scint compatibility -->
      <property name="FASTCOMPONENT" ref="SCINT_SPECTRUM"/>
      <property name="SLOWCOMPONENT" ref="SCINT_SPECTRUM"/>
      <property name="FASTTIMECONSTANT" ref="FAST_TIME_CONST"/>
      <property name="SLOWTIMECONSTANT" ref="SLOW_TIME_CONST"/>
      <property name="YIELDRATIO" ref="YIELD_RATIO"/>
      <property name="REEMISSIONPROB" ref="REEMISSION_PROB"/>
    </material>

    <!-- Optical Grease -->
    <material name="OpticalGrease" state="solid">
      <D unit="g/cm3" value="1.03"/>
      <fraction n="1.0" ref="G4_SILICON_DIOXIDE"/>
      <property name="RINDEX" ref="RINDEX_GREASE"/>
      <property name="ABSLENGTH" ref="ABSLENGTH_GREASE"/>
    </material>

    <!-- Entrance Window -->
    <material name="EntranceWindow" state="solid">
      <D unit="g/cm3" value="1.2"/>
      <fraction n="1.0" ref="G4_SILICON_DIOXIDE"/>
      <property name="RINDEX" ref="RINDEX_WINDOW"/>
      <property name="ABSLENGTH" ref="ABSLENGTH_WINDOW"/>
    </material>
  </materials>

  <solids>
    <!-- World -->
    <box name="WorldBox" x="100" y="100" z="100" lunit="mm"/>
    
    <!-- Crystal pixel (8mm thick) -->
    <box name="CrystalPixel" x="2.0" y="2.0" z="8.0" lunit="mm"/>
    
    <!-- Optical grease layer (covers entire array) -->
    <box name="GreaseLayer" x="17.4" y="17.4" z="0.1" lunit="mm"/>
    
    <!-- Entrance window (covers entire array) -->
    <box name="WindowLayer" x="17.4" y="17.4" z="0.1" lunit="mm"/>
    
    <!-- SiPM active area (0.2mm thick) -->
    <box name="SiPMActive" x="2.0" y="2.0" z="0.2" lunit="mm"/>
    
    <!-- SiPM horizontal dead space (between rows) -->
    <box name="SiPMDeadH" x="17.4" y="0.2" z="0.2" lunit="mm"/>
    
    <!-- SiPM vertical dead space (between columns) -->
    <box name="SiPMDeadV" x="0.2" y="2.0" z="0.2" lunit="mm"/>
    
    <!--OPTICAL SURFACES -->
    
    <!-- 100% QE -->
    <opticalsurface name="DetectorSurface" type="dielectric_dielectric" 
                   finish="polished" model="glisur" value="1">
      <property name="REFLECTIVITY" ref="REFLECTIVITY_0"/>
      <property name="EFFICIENCY" ref="EFFICIENCY_100"/>
    </opticalsurface>
    
    <!-- FIN2 & FIN3: Crystal side surfaces (POLISH, 0.98RC) -->
    <opticalsurface name="CrystalSideSurface" type="dielectric_dielectric" 
                   finish="polished" model="glisur" value="1">
      <property name="REFLECTIVITY" ref="REFLECTIVITY_98"/>
      <property name="EFFICIENCY" ref="EFFICIENCY_0"/>
    </opticalsurface>
    
    <!-- FIN4: MPPC dead space (METAL, 0.3RC) -->
    <opticalsurface name="DeadSurface" type="dielectric_dielectric" 
                   finish="polished" model="glisur" value="1">
      <property name="REFLECTIVITY" ref="REFLECTIVITY_30"/>
      <property name="EFFICIENCY" ref="EFFICIENCY_0"/>
    </opticalsurface>
    
    <!-- FIN5: Smooth coupling surface (POLISH) -->
    <opticalsurface name="CouplingSurface" type="dielectric_dielectric" 
                   finish="polished" model="glisur" value="1">
      <property name="REFLECTIVITY" ref="REFLECTIVITY_0"/>
      <property name="EFFICIENCY" ref="EFFICIENCY_0"/>
    </opticalsurface>
    
    <!-- FIN6: Crystal top surface (POLISH, 0.98RC) -->
    <opticalsurface name="CrystalTopSurface" type="dielectric_dielectric" 
                   finish="polished" model="glisur" value="1">
      <property name="REFLECTIVITY" ref="REFLECTIVITY_98"/>
      <property name="EFFICIENCY" ref="EFFICIENCY_0"/>
    </opticalsurface>
  </solids>

  <structure>
    <!-- ========== VOLUME DEFINITIONS ========== -->
    
    <!-- SiPM Active Area Volume - THIS IS THE SENSITIVE DETECTOR -->
    <volume name="SiPMActiveVol">
      <materialref ref="EntranceWindow"/>
      <solidref ref="SiPMActive"/>
      <!--  SensDet tag for Opticks sensitivity -->
      <auxiliary auxtype="SensDet" auxvalue="SD0"/>
      <auxiliary auxtype="SensorSurface" auxvalue="DetectorSurface"/>
      <auxiliary auxtype="OpticalSurface" auxvalue="DetectorSurface"/>
      <auxiliary auxtype="Optical" auxvalue="1"/>
      <auxiliary auxtype="GroupName" auxvalue="SiPMActive"/>
    </volume>
    
    <!-- SiPM Horizontal Dead Space Volume -->
    <volume name="SiPMDeadHVol">
      <materialref ref="EntranceWindow"/>
      <solidref ref="SiPMDeadH"/>
      <auxiliary auxtype="OpticalSurface" auxvalue="DeadSurface"/>
      <auxiliary auxtype="Optical" auxvalue="1"/>
      <auxiliary auxtype="GroupName" auxvalue="SiPMDead"/>
    </volume>
    
    <!-- SiPM Vertical Dead Space Volume -->
    <volume name="SiPMDeadVVol">
      <materialref ref="EntranceWindow"/>
      <solidref ref="SiPMDeadV"/>
      <auxiliary auxtype="OpticalSurface" auxvalue="DeadSurface"/>
      <auxiliary auxtype="Optical" auxvalue="1"/>
      <auxiliary auxtype="GroupName" auxvalue="SiPMDead"/>
    </volume>
    
    <!-- Crystal Pixel Volume -->
    <volume name="CrystalPixelVol">
      <materialref ref="Crystal"/>
      <solidref ref="CrystalPixel"/>
      <auxiliary auxtype="Optical" auxvalue="1"/>
      <auxiliary auxtype="SkinSurface" auxvalue="CrystalSideSurface"/>
      <auxiliary auxtype="GroupName" auxvalue="CrystalPixels"/>
    </volume>
    
    <!-- Optical Grease Volume -->
    <volume name="GreaseVol">
      <materialref ref="OpticalGrease"/>
      <solidref ref="GreaseLayer"/>
      <auxiliary auxtype="Optical" auxvalue="1"/>
    </volume>
    
    <!-- Entrance Window Volume -->
    <volume name="WindowVol">
      <materialref ref="EntranceWindow"/>
      <solidref ref="WindowLayer"/>
      <auxiliary auxtype="Optical" auxvalue="1"/>
    </volume>
    
    <!-- ========== WORLD VOLUME WITH COMPLETE 8x8 ARRAY ========== -->
    <volume name="WorldLog">
      <materialref ref="Air"/>
      <solidref ref="WorldBox"/>
      <auxiliary auxtype="Optical" auxvalue="1"/>
      
      <!-- Place Grease Layer (covers entire array) -->
      <physvol name="GreaseLayer">
        <volumeref ref="GreaseVol"/>
        <position name="grease_pos" x="0" y="0" z="8.05" unit="mm"/>
      </physvol>
      
      <!-- Place Entrance Window (covers entire array) -->
      <physvol name="WindowLayer">
        <volumeref ref="WindowVol"/>
        <position name="window_pos" x="0" y="0" z="8.15" unit="mm"/>
      </physvol>
      
      <!-- ========== COMPLETE 8x8 CRYSTAL ARRAY (64 CRYSTALS) ========== -->
      <!-- Row 0 (y = -8.7mm) -->
      <physvol name="Crystal_0_0">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_0_0" x="-8.7" y="-8.7" z="4.0" unit="mm"/>
      </physvol>
      <physvol name="Crystal_0_1">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_0_1" x="-6.5" y="-8.7" z="4.0" unit="mm"/>
      </physvol>
      <physvol name="Crystal_0_2">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_0_2" x="-4.3" y="-8.7" z="4.0" unit="mm"/>
      </physvol>
      <physvol name="Crystal_0_3">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_0_3" x="-2.1" y="-8.7" z="4.0" unit="mm"/>
      </physvol>
      <physvol name="Crystal_0_4">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_0_4" x="0.1" y="-8.7" z="4.0" unit="mm"/>
      </physvol>
      <physvol name="Crystal_0_5">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_0_5" x="2.3" y="-8.7" z="4.0" unit="mm"/>
      </physvol>
      <physvol name="Crystal_0_6">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_0_6" x="4.5" y="-8.7" z="4.0" unit="mm"/>
      </physvol>
      <physvol name="Crystal_0_7">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_0_7" x="6.7" y="-8.7" z="4.0" unit="mm"/>
      </physvol>
      
      <!-- Row 1 (y = -6.5mm) -->
      <physvol name="Crystal_1_0">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_1_0" x="-8.7" y="-6.5" z="4.0" unit="mm"/>
      </physvol>
      <physvol name="Crystal_1_1">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_1_1" x="-6.5" y="-6.5" z="4.0" unit="mm"/>
      </physvol>
      <physvol name="Crystal_1_2">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_1_2" x="-4.3" y="-6.5" z="4.0" unit="mm"/>
      </physvol>
      <physvol name="Crystal_1_3">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_1_3" x="-2.1" y="-6.5" z="4.0" unit="mm"/>
      </physvol>
      <physvol name="Crystal_1_4">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_1_4" x="0.1" y="-6.5" z="4.0" unit="mm"/>
      </physvol>
      <physvol name="Crystal_1_5">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_1_5" x="2.3" y="-6.5" z="4.0" unit="mm"/>
      </physvol>
      <physvol name="Crystal_1_6">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_1_6" x="4.5" y="-6.5" z="4.0" unit="mm"/>
      </physvol>
      <physvol name="Crystal_1_7">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_1_7" x="6.7" y="-6.5" z="4.0" unit="mm"/>
      </physvol>
      
      <!-- Row 2 (y = -4.3mm) -->
      <physvol name="Crystal_2_0">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_2_0" x="-8.7" y="-4.3" z="4.0" unit="mm"/>
      </physvol>
      <physvol name="Crystal_2_1">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_2_1" x="-6.5" y="-4.3" z="4.0" unit="mm"/>
      </physvol>
      <physvol name="Crystal_2_2">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_2_2" x="-4.3" y="-4.3" z="4.0" unit="mm"/>
      </physvol>
      <physvol name="Crystal_2_3">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_2_3" x="-2.1" y="-4.3" z="4.0" unit="mm"/>
      </physvol>
      <physvol name="Crystal_2_4">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_2_4" x="0.1" y="-4.3" z="4.0" unit="mm"/>
      </physvol>
      <physvol name="Crystal_2_5">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_2_5" x="2.3" y="-4.3" z="4.0" unit="mm"/>
      </physvol>
      <physvol name="Crystal_2_6">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_2_6" x="4.5" y="-4.3" z="4.0" unit="mm"/>
      </physvol>
      <physvol name="Crystal_2_7">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_2_7" x="6.7" y="-4.3" z="4.0" unit="mm"/>
      </physvol>
      
      <!-- Row 3 (y = -2.1mm) -->
      <physvol name="Crystal_3_0">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_3_0" x="-8.7" y="-2.1" z="4.0" unit="mm"/>
      </physvol>
      <physvol name="Crystal_3_1">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_3_1" x="-6.5" y="-2.1" z="4.0" unit="mm"/>
      </physvol>
      <physvol name="Crystal_3_2">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_3_2" x="-4.3" y="-2.1" z="4.0" unit="mm"/>
      </physvol>
      <physvol name="Crystal_3_3">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_3_3" x="-2.1" y="-2.1" z="4.0" unit="mm"/>
      </physvol>
      <physvol name="Crystal_3_4">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_3_4" x="0.1" y="-2.1" z="4.0" unit="mm"/>
      </physvol>
      <physvol name="Crystal_3_5">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_3_5" x="2.3" y="-2.1" z="4.0" unit="mm"/>
      </physvol>
      <physvol name="Crystal_3_6">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_3_6" x="4.5" y="-2.1" z="4.0" unit="mm"/>
      </physvol>
      <physvol name="Crystal_3_7">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_3_7" x="6.7" y="-2.1" z="4.0" unit="mm"/>
      </physvol>
      
      <!-- Row 4 (y = 0.1mm) -->
      <physvol name="Crystal_4_0">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_4_0" x="-8.7" y="0.1" z="4.0" unit="mm"/>
      </physvol>
      <physvol name="Crystal_4_1">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_4_1" x="-6.5" y="0.1" z="4.0" unit="mm"/>
      </physvol>
      <physvol name="Crystal_4_2">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_4_2" x="-4.3" y="0.1" z="4.0" unit="mm"/>
      </physvol>
      <physvol name="Crystal_4_3">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_4_3" x="-2.1" y="0.1" z="4.0" unit="mm"/>
      </physvol>
      <physvol name="Crystal_4_4">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_4_4" x="0.1" y="0.1" z="4.0" unit="mm"/>
      </physvol>
      <physvol name="Crystal_4_5">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_4_5" x="2.3" y="0.1" z="4.0" unit="mm"/>
      </physvol>
      <physvol name="Crystal_4_6">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_4_6" x="4.5" y="0.1" z="4.0" unit="mm"/>
      </physvol>
      <physvol name="Crystal_4_7">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_4_7" x="6.7" y="0.1" z="4.0" unit="mm"/>
      </physvol>
      
      <!-- Row 5 (y = 2.3mm) -->
      <physvol name="Crystal_5_0">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_5_0" x="-8.7" y="2.3" z="4.0" unit="mm"/>
      </physvol>
      <physvol name="Crystal_5_1">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_5_1" x="-6.5" y="2.3" z="4.0" unit="mm"/>
      </physvol>
      <physvol name="Crystal_5_2">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_5_2" x="-4.3" y="2.3" z="4.0" unit="mm"/>
      </physvol>
      <physvol name="Crystal_5_3">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_5_3" x="-2.1" y="2.3" z="4.0" unit="mm"/>
      </physvol>
      <physvol name="Crystal_5_4">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_5_4" x="0.1" y="2.3" z="4.0" unit="mm"/>
      </physvol>
      <physvol name="Crystal_5_5">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_5_5" x="2.3" y="2.3" z="4.0" unit="mm"/>
      </physvol>
      <physvol name="Crystal_5_6">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_5_6" x="4.5" y="2.3" z="4.0" unit="mm"/>
      </physvol>
      <physvol name="Crystal_5_7">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_5_7" x="6.7" y="2.3" z="4.0" unit="mm"/>
      </physvol>
      
      <!-- Row 6 (y = 4.5mm) -->
      <physvol name="Crystal_6_0">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_6_0" x="-8.7" y="4.5" z="4.0" unit="mm"/>
      </physvol>
      <physvol name="Crystal_6_1">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_6_1" x="-6.5" y="4.5" z="4.0" unit="mm"/>
      </physvol>
      <physvol name="Crystal_6_2">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_6_2" x="-4.3" y="4.5" z="4.0" unit="mm"/>
      </physvol>
      <physvol name="Crystal_6_3">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_6_3" x="-2.1" y="4.5" z="4.0" unit="mm"/>
      </physvol>
      <physvol name="Crystal_6_4">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_6_4" x="0.1" y="4.5" z="4.0" unit="mm"/>
      </physvol>
      <physvol name="Crystal_6_5">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_6_5" x="2.3" y="4.5" z="4.0" unit="mm"/>
      </physvol>
      <physvol name="Crystal_6_6">
        <volumeref ref="CrystalPixelVol"/>
        <position name="crystal_pos_6_6" x="4.5" y="4.5" z="4.0" unit="mm"/>
      </physvol>
